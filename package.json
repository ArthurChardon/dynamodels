{
  "name": "dynamodel",
  "version": "0.0.7",
  "description": "Typescript Overlay to easily manage AWS DynamoDB entities",
  "main": "dist/index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "pre:test": "npx sls dynamodb install",
    "test": "LOCAL_DYNAMODB_PORT=8022 npx jest --runInBand --verbose --colors --config jest.config.json --detectOpenHandles",
    "build": "npx tsc"
  },
  "author": "Mario Arnautou",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "aws-sdk": "^2.520.0"
  },
  "devDependencies": {
    "@types/hapi__joi": "^15.0.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "jest": "^24.9.0",
    "kill-port": "^1.5.1",
    "serverless": "^1.51.0",
    "serverless-dynamodb-local": "^0.2.38",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  }
}
